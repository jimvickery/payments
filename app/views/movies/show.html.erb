<div class="page-header">
  <%= link_to movies_path, class: 'btn btn-default' do %>
    All Movies
  <% end %>
  <%= link_to edit_movie_path(@movie), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show movie</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @movie.title %></dd>

  <dt>Video url:</dt>
  <dd><%= @movie.video_url %></dd>

</dl>
